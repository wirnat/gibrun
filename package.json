{
    "name": "gibrun-mcp-server",
    "version": "1.0.0",
    "description": "MCP Server untuk backend testing dengan PostgreSQL, HTTP requests, dan Go build automation",
    "type": "module",
    "main": "build/index.js",
    "bin": {
        "gibrun-mcp-server": "./build/index.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "postbuild": "node scripts/postbuild.mjs",
        "prepare": "npm run build",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest --watch"
    },
    "keywords": [
        "mcp",
        "server",
        "api-testing",
        "postgresql",
        "go-build"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.0.4",
        "axios": "^1.7.9",
        "pg": "^8.13.1"
    },
    "devDependencies": {
        "@types/node": "^22.19.1",
        "@types/pg": "^8.11.10",
        "@vitest/ui": "^4.0.8",
        "@vscode/debugprotocol": "^1.68.0",
        "jsdom": "^27.2.0",
        "typescript": "^5.7.2",
        "vitest": "^4.0.8"
    }
}
