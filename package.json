{
  "name": "gibrun-mcp-server",
  "version": "1.1.0",
  "description": "MCP Server untuk backend testing dengan PostgreSQL, HTTP requests, dan Go build automation",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "gibrun-mcp-server": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "dev": "tsc --watch",
    "postbuild": "node scripts/postbuild.mjs",
    "prepare": "npm run build",
    "start": "node build/index.js",
    "docker:build": "docker build -t gibrun-mcp-server .",
    "docker:run": "docker run --rm gibrun-mcp-server",
    "docker:compose:up": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:compose:down": "docker-compose -f docker-compose.prod.yml down",
    "lint": "eslint src/ test/ --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src/ test/ --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\" \"test/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\" \"test/**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
        "test:integration": "vitest test/integration",
        "test:integration:secure": "TEST_SECURE_MODE=1 vitest test/integration",
    "test:unit": "vitest test/unit",
    "test:dap": "vitest test/unit/tools/dap",
    "test:services": "vitest test/unit/services",
    "test:tools": "vitest test/unit/tools",
    "test:sequential": "NODE_OPTIONS='--max-old-space-size=4096' vitest run --no-coverage --sequence.concurrent=false",
    "test:split": "npm run test:unit:dap && npm run test:unit:services && npm run test:unit:tools && npm run test:integration",
    "test:unit:dap": "NODE_OPTIONS='--max-old-space-size=2048' vitest run test/unit/tools/dap --no-coverage",
    "test:unit:services": "NODE_OPTIONS='--max-old-space-size=2048' vitest run test/unit/services --no-coverage",
    "test:unit:tools": "NODE_OPTIONS='--max-old-space-size=2048' vitest run test/unit/tools --no-coverage",
    "test:memory-safe": "node scripts/test-runner.mjs",
    "test:performance": "vitest test/performance",
    "benchmark": "node benchmark.js",
    "security:audit": "npm audit",
    "security:audit:fix": "npm audit fix",
    "clean": "rm -rf build/ coverage/ *.log",
    "prepublishOnly": "npm run build && npm run test:run",
    "ci": "npm run lint && npm run typecheck && npm run test:run && npm run build"
  },
  "keywords": [
    "mcp",
    "server",
    "api-testing",
    "postgresql",
    "go-build"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.7.9",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/node": "^22.19.1",
    "@types/pg": "^8.11.10",
    "@vitest/ui": "^4.0.8",
    "@vscode/debugprotocol": "^1.68.0",
    "dotenv": "^17.2.3",
    "jsdom": "^27.2.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.8"
  }
}
